2]","written_form":"n","cotext_translation":"[Dativ: Richtung]","lemma_form":"n","lemmaID":"78870","pos":"preposition","token":"oraec674-2-5"},{"lineCount":" [recto,2]","written_form":"=k","cotext_translation":"[Suffix Pron. sg.2.m.]","lemma_form":"=k","lemmaID":"10110","pos":"pronoun","pronoun":"personal_pronoun","token":"oraec674-2-6"},{"lineCount":" [recto,3]","written_form":"ḥr","cotext_translation":"in Hinsicht auf (Bezug)","lemma_form":"ḥr","lemmaID":"107520","pos":"preposition","token":"oraec674-2-7"},{"lineCount":" [recto,3]","written_form":"tꜣy","cotext_translation":"[Poss.artikel sg.f.]","lemma_form":"tꜣy=","lemmaID":"550046","pos":"pronoun","pronoun":"demonstrative_pronoun","token":"oraec674-2-8"},{"lineCount":" [recto,3]","written_form":"=k","cotext_translation":"[Suffix Pron. sg.2.m.]","lemma_form":"=k","lemmaID":"10110","pos":"pronoun","pronoun":"personal_pronoun","token":"oraec674-2-9"},{"lineCount":" [recto,3]","written_form":"s,t","cotext_translation":"(berufliche) Stellung","lemma_form":"s.t","lemmaID":"854540","pos":"substantive","numerus":"singular","status":"st_absolutus","genus":"feminine","token":"oraec674-2-10"}],"translation":"Verhindere, daß ich dich dich in deiner amtlichen Position tadeln muß."},{"token":[{"lineCount":" [recto,3]","written_form":"m","cotext_translation":"nicht (Imperativ des Negativverbs jmi̯)","lemma_form":"m","lemmaID":"64410","pos":"verb","token":"oraec674-3-1"},{"lineCount":" [recto,3]","written_form":"ngꜣ","cotext_translation":"mangeln","lemma_form":"ngꜣ","lemmaID":"89530","pos":"verb","verbalClass":"verb_3-lit","inflection":"infinitive","token":"oraec674-3-2"},{"lineCount":" [recto,3]","written_form":"s","cotext_translation":"sie [Enkl. Pron. sg.3.f.]","lemma_form":"sj","lemmaID":"127770","pos":"pronoun","pronoun":"personal_pronoun","token":"oraec674-3-3"},{"lineCount":" [recto,3]","written_form":"m-šs","cotext_translation":"in gutem Zustand","lemma_form":"m-šs","lemmaID":"600392","pos":"adverb","token":"oraec674-3-4"},{"lineCount":" [recto,3]","written_form":"zp-2","cotext_translation":"zweimal (Betonung beim Imperativ, Adjektiven etc.)","lemma_form":"zp-2","lemmaID":"70011","pos":"substantive","status":"st_absolutus","numerus":"singular","token":"oraec674-3-5"}],"translation":"Laß es ihr (=der Tätigkeit im Amt) nicht an irgendeinem Erfolg mangeln."},{"token":[{"lineCount":" [recto,3]","written_form":"ḥnꜥ","cotext_translation":"ferner","lemma_form":"ḥnꜥ","lemmaID":"850800","pos":"preposition","token":"oraec674-4-1"},{"lineCount":" [recto,3]","written_form":"ntk","cotext_translation":"[Selbst. Pron. sg.2.m.]","lemma_form":"jntk","lemmaID":"90120","pos":"pronoun","pronoun":"personal_pronoun","token":"oraec674-4-2"},{"lineCount":" [recto,3]","written_form":"ṯꜣi̯.t","cotext_translation":"fortnehmen","lemma_form":"ṯꜣi̯","lemmaID":"174260","pos":"verb","verbalClass":"verb_3-inf","inflection":"infinitive","morphology":"t-morpheme","token":"oraec674-4-3"},{"lineCount":" [recto,3]","written_form":"n","cotext_translation":"[Dativ: Nutzen]","lemma_form":"n","lemmaID":"78870","pos":"preposition","token":"oraec674-4-4"},{"lineCount":" [recto,3]","written_form":"=j","cotext_translation":"[Suffix Pron. sg.1.c.]","lemma_form":"=j","lemmaID":"10030","pos":"pronoun","pronoun":"personal_pronoun","token":"oraec674-4-5"},{"lineCount":" [recto,3]","written_form":"sm,w","cotext_translation":"Kraut","lemma_form":"sm.w","lemmaID":"134140","pos":"substantive","numerus":"singular","status":"st_absolutus","genus":"masculine","token":"oraec674-4-6"},{"lineCount":" [recto,3]","written_form":"qn,w","cotext_translation":"zahlreich","lemma_form":"qn.w","lemmaID":"161060","pos":"adjective","token":"oraec674-4-7"},{"lineCount":" [recto,3]","written_form":"nḫb.w","cotext_translation":"Lotusblüte","lemma_form":"nḫb","lemmaID":"87020","pos":"substantive","numerus":"plural","status":"st_absolutus","token":"oraec674-4-8"},{"lineCount":" [recto,3]","written_form":"ḥrr,t","cotext_translation":"Blume","lemma_form":"ḥr